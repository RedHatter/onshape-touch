{
  "compilerOptions": {
    "target": "esnext",
    "module": "none",
    "noResolve": true,

    "inlineSourceMap": true,
    "alwaysStrict": false,

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,

    "skipLibCheck": true
  }
}
